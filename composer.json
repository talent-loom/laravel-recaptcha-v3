{
    "name": "talent-loom/laravel-recaptcha-v3",
    "description": "PHP package for reCAPTCHA v3 in Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TalentLoom\\LaravelRecaptchaV3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TalentLoom\\LaravelRecaptchaV3\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Syed Shazeedul Islam",
            "email": "syedshazeedul@gmail.com",
            "role": "Developer",
            "homepage": "https://github.com/talant-loom"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.35",
        "orchestra/testbench": "^8.0",
        "laravel/pint": "^1.17"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TalentLoom\\LaravelRecaptchaV3\\Providers\\RecaptchaV3ServiceProvider"
            ],
            "aliases": {
                "RecaptchaV3": "TalentLoom\\LaravelRecaptchaV3\\Facades\\RecaptchaV3"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}